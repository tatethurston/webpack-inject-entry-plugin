{
  "name": "webpack-inject-entry-plugin",
  "version": "0.0.4",
  "description": "A webpack plugin to inject code into the bundle.",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/webpack-inject-entry-plugin"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "pnpm typecheck && prettier --check . && prettier-package-json --list-different '{,example/,}package.json' && eslint .",
    "lint:fix": "prettier --write . && prettier-package-json --write '{,example/}package.json' && eslint --fix .",
    "package:build": "pnpm clean && pnpm tsc",
    "package:prune": "rm dist/**/test.{d.ts,js}",
    "test": "pnpm jest",
    "typecheck": "pnpm tsc --noEmit",
    "version": "pnpm run build && git add -A package.json",
    "postversion": "git push && git push --tags"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "dependencies": {
    "schema-utils": "^4.0.0"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "prettier-package-json": "^2.8.0",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "entry",
    "inject",
    "plugin",
    "webpack"
  ],
  "packageManager": "pnpm@7.17.0"
}
